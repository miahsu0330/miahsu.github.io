{
  "name": "miahsuwork",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npm run lint:styles",
    "lint:styles": "stylelint \"src/**/*.css\" --fix --max-warnings 0",
    "prettier": "prettier '{,!(.next)/**/}**/*.{js,jsx,ts,tsx,css,json}' --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,json,md}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "**/*.{css}": [
      "stylelint --fix --max-warnings 0"
    ]
  },
  "dependencies": {
    "@gsap/react": "^2.1.0",
    "@headlessui/react": "^1.7.18",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.15.13",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "formik": "^2.2.9",
    "gsap": "^3.12.5",
    "next": "^14.1.1",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-use": "^17.5.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.33.2",
    "swiper": "^11.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "5.0.4",
    "yup": "^1.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0"
  }
}
